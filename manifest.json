{
   "content_scripts": [ {
      "js": [ "inject.js" ],
      "matches": [ "https://*/*" ],
      "all_frames": true,
      "match_about_blank": true,
      "run_at": "document_start"
   } ],
   "browser_action": {
     "default_icon": "icon.png",
	 "default_title": "Debug WebRTC"
   },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "manifest_version": 2,
  "name": "WebRTC Externals",
  "author": "Philipp Hancke",
  "version": "1.0.0",
  "icons": {
    "128": "logo.png"
  }
}
